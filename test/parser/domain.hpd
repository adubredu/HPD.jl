(define (domain gripper)
  (:predicates (room ?r) (ball ?b) (gripper ?g) (robbyat ?r)
               (at ?b ?r) (free ?g) (carry ?o ?g))
  (:action move
   :parameters (?from ?to)
   :precondition (and (room ?from)
                      (room ?to)
                      (robbyat ?from))
   :continuous_precondition (math (|xr-xo|**2+|yr-yo|**2<=0.25))
   :dynamics ();x==4x+4) (y==5x+8))
   :continuous_effect (math (8x+4y<=17) (3x+7y<=19) (3x+3y==78))
   :effect (and (robbyat ?to)
                (not (robbyat ?from))))

(:action pick
    :parameters (?obj)
    :precondition (and  (handempty) (clear ?obj))
    :continuous_precondition  ();|xr-xo|**2+|yr-yo|**2<=0.25))
    :dynamics (math (x==vx*t) (y==vy*t))
    :continuous_effect (math (xo==xr) (yo==yr))
    :effect (and  (holding ?obj) 
                  (not (clear ?obj)) (not (handempty))))

  (:action drop
   :parameters (?obj ?room ?gripper)
   :precondition (and (ball ?obj)
                      (room ?room)
                      (gripper ?gripper)
                      (carry ?obj ?gripper)
                      (robbyat ?room))
   :continuous_precondition (math (3x+4y==16) (3x+4y<=16))
   :dynamics (math (x==4x+4) (y==5x+8))
   :continuous_effect (math (8x+4y<=17) (3x+7y<=19) (3x+3y==78))
   :effect (and (at ?obj ?room)
                (free ?gripper)
                (not (carry ?obj ?gripper)))))